<Project>
	<PropertyGroup>
		<Title>$(Product)</Title>
		<Authors>Andrey Taritsyn</Authors>
		<PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
		<PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
		<PackageProjectUrl>https://github.com/Taritsyn/BundleTransformer</PackageProjectUrl>
		<RepositoryUrl>https://github.com/Taritsyn/BundleTransformer</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<NeutralLanguage>en-US</NeutralLanguage>
	</PropertyGroup>

	<ItemGroup>
		<Content Include="../../LICENSE.txt">
			<PackagePath>$(PackageLicenseFile)</PackagePath>
			<Pack>true</Pack>
			<Visible>false</Visible>
		</Content>
	</ItemGroup>

	<Import Project="nuget-common.props" />
</Project>